# Firebase Configuration
VITE_FIREBASE_API_KEY=your-api-key-here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdefghijklmnop
VITE_FIREBASE_MEASUREMENT_ID=G-ABCDEFGHIJ

# FCM VAPID Key (for push notifications)
VITE_FCM_VAPID_KEY=your-vapid-key-here

# App Configuration
VITE_APP_NAME="PWA Template"
VITE_APP_DESCRIPTION="PWA Bootstrap Template"

# Development Settings
# Set to 'true' to use Firebase emulators in development
VITE_USE_FIREBASE_EMULATORS=false